<MODEL>
    <!-- 符号名称，如 地震 -->
    <CAPTION>大雨</CAPTION>  
    <!-- 符号描述 如 地震是***  -->
    <DESCRIPTION>24小时内降雨量达25.0~49.9毫米之间的雨称为大雨</DESCRIPTION>
    <!-- 符号的文件名，如 a.gltf -->
    <FILE>大雨.gltf</FILE>
    <!-- 缩略图名，如 a.gif -->
    <THUMBNAIL>大雨.gif</THUMBNAIL>
    <!-- 符号支持的参数，不清楚可先不填 -->
    <PARAMETERS>
        <PARAMETER>
            <NAME></NAME>
            <MEAN></MEAN>
            <TYPE></TYPE>
            <DEFAULTVALUE></DEFAULTVALUE>
            <MINVALUE></MINVALUE>
            <MAXVALUE></MAXVALUE>
            <CATEGORY></CATEGORY>
        </PARAMETER>
        <PARAMETER></PARAMETER>
    </PARAMETERS>
</MODEL>
